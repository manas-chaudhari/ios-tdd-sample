  target 'ArchitectureSampleTests' do
    inherit! :search_paths
    # Pods for testing

    pod 'Quick'
    pod 'Nimble'
  end

  target 'ArchitectureSampleUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
